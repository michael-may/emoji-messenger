{
  "name": "emoji-messenger",
  "description": "Send and receive encrypted messages.",
  "version": "0.9.5",
  "author": "Michael May",
  "scripts": {
    "ng": "ng",
    "config": "ts-node src/environments/set-env",
    "parseEmojiList": "ts-node tools/emoji-parser",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "start": "node server.js",
    "start:dev": "npm run config && npm run helpers && ng serve --disable-host-check",
    "build": "npm run config && npm run helpers && ng build --configuration production --aot --build-optimizer=false",
    "build:pages": "ng build --output-path docs --base-href /emoji-messenger/",
    "test": "npm run config && npm run helpers && ng test",
    "build:dev": "npm run config && npm run helpers && ng build",
    "helpers": "node ./helpers/write-version.js",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^17.2.3",
    "@angular/core": "^17.2.3",
    "@angular/forms": "^17.2.3",
    "@angular/platform-browser": "^17.2.3",
    "@angular/platform-browser-dynamic": "^17.2.3",
    "@angular/router": "^17.2.3",
    "@angular/service-worker": "^17.2.3",
    "@ionic/angular": "^7.7.3",
    "compression": "^1.7.4",
    "core-js": "^3.20.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "helmet": "^3.21.2",
    "jsqr": "^1.4.0",
    "qrcode": "^1.4.4",
    "rxjs": "^7.8.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1702.2",
    "@angular-devkit/build-angular": "^17.2.2",
    "@angular-devkit/core": "^17.2.2",
    "@angular-devkit/schematics": "^17.2.2",
    "@angular/cli": "^17.2.2",
    "@angular/compiler": "^17.2.3",
    "@angular/compiler-cli": "^17.2.3",
    "@angular/language-service": "^17.2.3",
    "@ionic/angular-toolkit": "^11.0.1",
    "@schuchard/prettier": "^3.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^13.7.1",
    "codelyzer": "^6.0.0",
    "colors": "^1.4.0",
    "husky": "4.2.1",
    "jasmine-core": "^5.1.2",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "10.0.7",
    "prettier": "1.19.1",
    "protractor": "^7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~5.3.3"
  }
}
